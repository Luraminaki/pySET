{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "*.egg-info": true
    },
    "breadcrumbs.enabled": true,
    "editor.formatOnSave": false,
    "git.autofetch": true,
    "pylint.args": [
        "--disable=C0103, C0111, C0209, C0301, C0415, E0401, R0201, R0902, R0903, R0904, R0913, R0914, R0915, W0102, W1202, W1203, W0702, W0703",
        "--extension-pkg-whitelist=cv2",
        "--generated-members=cv2.*"
    ],
    "explorer.confirmDelete": false,
    "files.associations": {
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "cstddef": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "strstream": "cpp",
        "*.tcc": "cpp",
        "chrono": "cpp",
        "complex": "cpp",
        "condition_variable": "cpp",
        "cstdint": "cpp",
        "cwctype": "cpp",
        "deque": "cpp",
        "forward_list": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "exception": "cpp",
        "fstream": "cpp",
        "functional": "cpp",
        "initializer_list": "cpp",
        "iomanip": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "memory": "cpp",
        "mutex": "cpp",
        "new": "cpp",
        "ostream": "cpp",
        "numeric": "cpp",
        "ratio": "cpp",
        "sstream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "system_error": "cpp",
        "thread": "cpp",
        "cinttypes": "cpp",
        "type_traits": "cpp",
        "tuple": "cpp",
        "typeindex": "cpp",
        "typeinfo": "cpp",
        "utility": "cpp",
        "cstdarg": "cpp",
        "bitset": "cpp",
        "optional": "cpp",
        "string_view": "cpp",
        "unordered_set": "cpp",
        "list": "cpp",
        "valarray": "cpp",
        "variant": "cpp"
    },
    "python.testing.pytestArgs": [
        ""
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": false,
    "python.testing.promptToConfigure": false,
    "python.languageServer": "Pylance",
    "typescript.tsdk": "node_modules/typescript/lib"
}
